#ifndef __MBED_EDGE_BUILD_BUILD_INFO_H__
#define __MBED_EDGE_BUILD_BUILD_INFO_H__
//  build info, #include MBED_EDGE_BUILD_BUILD_INFO_HEADER to access
#define MBED_EDGE_BUILD_BUILD_YEAR 2018 // UTC year
#define MBED_EDGE_BUILD_BUILD_MONTH 4 // UTC month 1-12
#define MBED_EDGE_BUILD_BUILD_DAY 27 // UTC day 1-31
#define MBED_EDGE_BUILD_BUILD_HOUR 6 // UTC hour 0-24
#define MBED_EDGE_BUILD_BUILD_MINUTE 28 // UTC minute 0-59
#define MBED_EDGE_BUILD_BUILD_SECOND 41 // UTC second 0-61
#define MBED_EDGE_BUILD_BUILD_UUID 2c91ff27-2bd1-4f4c-90fb-20333b7b1f1d // unique random UUID for each build
#define MBED_EDGE_BUILD_BUILD_VCS_ID fde55f68a50d9a9f6378e1bd5bc299efbb355b9a // git or mercurial hash
#define MBED_EDGE_BUILD_BUILD_VCS_CLEAN 1 // evaluates true if the version control system was clean, otherwise false
#define MBED_EDGE_BUILD_BUILD_VCS_DESCRIPTION 0.4.5 // git describe or mercurial equivalent
#endif // ndef __MBED_EDGE_BUILD_BUILD_INFO_H__
